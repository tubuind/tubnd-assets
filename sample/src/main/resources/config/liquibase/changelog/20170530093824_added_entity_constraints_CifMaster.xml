        <addForeignKeyConstraint baseColumnNames="mtd_customergroup_id"
                                 baseTableName="cif_master"
                                 constraintName="cif_master_mtd_customerg_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mtd_customergroup"/>

    </changeSet>
</databaseChangeLog>
